package utils.camera{		import flash.media.*;	import flash.display.*;	import flash.geom.*;	//	import utils.capabilities.*;	import utilsMine;	//	import id.core.TouchSprite;					//		public function createSnapshot(video:Video, imgBD:BitmapData, imgBitmap:Bitmap, filterBMp:Bitmap, bmpsHolder:TouchSprite):TouchSprite {									imgBD = new BitmapData(video.width,video.height);			imgBD = utilsMine.snapClip( video );		   			imgBitmap = new Bitmap(imgBD);						bmpsHolder = new TouchSprite();			/////						if( isiPhone()){				filterBMp = new Bitmap(utilsMine.snapClip( imgBitmap ));				bmpsHolder.addChild(imgBitmap);				bmpsHolder.addChild(filterBMp);				//rectDrag = new Rectangle(filterBMp.x-300, filterBMp.y-200, 200, 200);							}			else{				imgBitmap.scaleX = -1;				bmpsHolder.addChild(imgBitmap);				//rectDrag = new Rectangle(imgBitmap.x-imgBitmap.width-300, imgBitmap.y-200, 200, 200);			}						return bmpsHolder;					}					}